<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="BigStore.DoDesignWS"
                   namespace="BigStore.DoDesignWS.Model"
                   default-lazy="false">
